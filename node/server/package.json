{
  "name": "server",
  "private": true,
  "version": "0.0.0",
  "description": "Api Server for Homing Pigeon.",
  "license": "MIT",
  "scripts": {
    "build": "prisma generate && prisma db push --accept-data-loss && nest build",
    "format": "prettier --write \"**/*.ts\"",
    "dev": "prisma generate && nest start",
    "dev:watch": "prisma generate && nest build --webpack --webpackPath webpack-hmr.config.cjs --watch",
    "dev:debug": "prisma generate && nest start --debug --watch",
    "start": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --detectOpenHandles --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/platform-socket.io": "^10.3.8",
    "@nestjs/platform-ws": "^10.3.8",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/websockets": "^10.3.8",
    "@prisma/client": "^5.13.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cache-manager": "^5.5.1",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compare-versions": "^6.1.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "http-status": "^1.7.4",
    "ioredis": "^5.4.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "nestjs-i18n": "^10.4.5",
    "nestjs-throttler-storage-redis": "^0.4.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "query-string": "7.1.3",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "sort-keys-recursive": "^2.1.10",
    "winston": "^3.13.0",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.0",
    "@types/node": "18.19.31",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/webpack-env": "^1.18.4",
    "@types/webpack-node-externals": "^3.0.4",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^5.13.0",
    "run-script-webpack-plugin": "^0.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-node-externals": "^3.0.0"
  },
  "packageManager": "pnpm@9.0.6",
  "engines": {
    "node": "^18"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest"
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/common/(.*)$": "<rootDir>/common/$1",
      "^@/modules/(.*)$": "<rootDir>/modules/$1"
    }
  }
}
